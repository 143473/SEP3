@page "/MyEvents"
@inject NavigationManager NavigationManager
<h3>MyEvents</h3>

<p class="actions">
    <button @onclick="@(() => CreateEventAsync())" class="oi oi-plus" type="submit" >Create</button>
</p>

@code {
    private async Task CreateEventAsync()
    {
        NavigationManager.NavigateTo($"CreateEvent");
    }
    
}