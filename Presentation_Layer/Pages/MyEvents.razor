@page "/MyEvents"
@inject NavigationManager NavigationManager
<h3>My Events</h3>

<p class="actions">
    <button @onclick="@(() => CreateEventAsync())" class="btn btn-outline-dark" type="submit">
        <i class="oi oi-plus"></i> Create
    </button>
</p>

@code {
    private async Task CreateEventAsync()
    {
        NavigationManager.NavigateTo($"CreateEvent");
    }
    
}